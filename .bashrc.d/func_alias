# FUNCTIONAL
alias l='ls -la'
alias search='grep'
alias upg='sudo dnf upgrade'
alias i='sudo dnf install'
alias vi='nvim'
alias c='clear'
alias rld='. ~/.bashrc.d/config'
alias chk='dnf check-update'
alias open='nvim .'

# APP RELATED
## Docker
alias start:docker='sudo systemctl --user start docker-desktop'
alias stop:docker='sudo systemctl --user stop docker-desktop'

# Node
alias pnx='pnpm exec'

